{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ],
    "crons": [
    {
      "path": "/src/main.py",
      "schedule": "0 10 * * 2"
    }
  ]
  }
  